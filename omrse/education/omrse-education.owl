        <obo:IAO_0000111 xml:lang="en">early childhood education program</obo:IAO_0000111>
        <obo:IAO_0000115 xml:lang="en">An educational program whose intended active participant, when a concretization of it is realized, is a human child of age birth to 8 years or a human fetus prior to birth.</obo:IAO_0000115>
        <obo:IAO_0000117 xml:lang="en">William R. Hogan</obo:IAO_0000117>
        <obo:IAO_0000119 xml:lang="en">https://apps.who.int/iris/bitstream/handle/10665/69729/a91213.pdf?sequence</obo:IAO_0000119>
        <rdfs:label xml:lang="en">early childhood education plan specification</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/OMRSE_00002034 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OMRSE_00002034">
